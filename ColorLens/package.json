    "@types/redux-thunk": "^2.1.0",
    "babel-jest": "^24.8.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "jest": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
