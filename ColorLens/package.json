    "lint": "eslint ."
  },
  "dependencies": {
    "axios": "^0.19.0",
    "react": "16.8.6",
    "react-native": "0.60.4",
    "react-native-camera": "^3.0.1",
    "react-native-gesture-handler": "^1.3.0",
